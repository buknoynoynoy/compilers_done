PROGRAM fibonacci
BEGIN

	STRING dummy := "abcde";

	INT i,result; 


	FUNCTION INT F (INT n)
	BEGIN

		IF (n > 2)
			RETURN F(n-1)+F(n-2);
		ENDIF
		IF (n = 0)   --This is a comment
			RETURN 0;
        ELSE
			RETURN 1;
		ENDIF
	END
	
	
	FUNCTION VOID main ()
	BEGIN
		
    INT i, end, result;
		READ(end);

	i := 0;
 	WHILE (i != end)
		result := F(i);
		WRITE (i);
		WRITE (result);
		i := i + 1;	
	ENDWHILE


	END

END	
